<div class="item-details">
  <h3><%= @item.category.name %></h3>
  <% @item.image_url.each do |image_url| %>
      <img src=<%= image_url %> class="card-img-top" width = "50px" height = "150px">
    <% end %>
  <h4><%= @item.name %><h4>
  <h4><%= @item.description %><h4>
  <h4><%= @item.price %><h4>
</div>

<%= link_to "Back to item list", items_path %>